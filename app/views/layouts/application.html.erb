    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <% unless @title.nil? %>
        	<% @title = "grubraise - " + @title %>
        <%end%>
         <title><%= @title %></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">

		<link rel="shortcut icon" href="assets/favicon.ico" type="image/x-icon" />
        <link rel="stylesheet" href="assets/normalize.min.css.scss">
        <link rel="stylesheet" href="assets/main.css.scss">

        <script src="assets/vendor/modernizr-2.6.1.min.js"></script>
    </head>

    <body>
        <!--[if lt IE 7]>
            <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
        <![endif]-->
		
		
		
		<!-- / ********************************************************************************************************************************************************************* -->
        <!-- / Start Wrap --><section id="wrap">
		
		<!-- / Start Content --><section id="content">
		
		<!-- / Start Header --><header>
							<section class="hold">
							<div id="error_messages" class="Logcontainer">
							<ul>
							<li><label for="email" class="error">Invalid username combination</label></li>
							<li><label for="password" class="error">Invalid password combination</label></li>            
							</ul>
							</div>
							</section>
		<!-- / Top Login Form -->
							<div class="login-form">
							<form action="" id="logform" method="post"  novalidate="novalidate">
							<fieldset class="right">
							<label for="email">Email   
							<input id="email" name="email" class="{validate:{required:true,email:true}}" type="text" value="">
							</label>
							<label for="password">Password
							<input id="password" name="password" class="{validate:{required:true}}" type="password">
							<a href="#" onclick="$('#password-box').toggle();">Forgot password<span></span></a>
							</label>
							<input type="submit" value="Login" class="btn" />
							</fieldset>
							</form>
							</div>
		
		<!-- / Forgot Password Box -->
							<div id="password-box" style="display:none">
							<div class="overlay"></div>
							<div class="forgotten-password-dialog" id="lightbox">
							<form action="" method="post" onsubmit="$.post($(this).attr('action'), $(this).serialize(), null, 'script'); return false;"><div style="margin:0;padding:0;display:inline"><input name="" type="hidden" value=""></div>
							<p> Enter your account email below and we'll send you a link to reset your password</p>
							<div id="password-errors">
							</div>
							<p class="email">
							<input id="forgotten_password_email" name="forgotten_password_email" type="text">
							</p>
							<div class="action">
							<input name="commit" type="submit" value="Send Link">
							</div>
							</form>
							<a class="remove" onclick="jQuery(this).parent().parent().toggle();">X<span></span></a>
							<span class="t"></span><span class="b"></span>
							</div>
							</div>

		
		<%= yield %>



		
							
							<footer>
							<p class="tel">Questions or comments? Call us at 646-633-4525</p>
						    <p><a href="help.html">Help</a> | <a href="privacy.html" onclick="window.open(this.href);return false;">Privacy Policy</a> | <a href="tos.html" onclick="window.open(this.href);return false;">Terms of Use</a> | <a href="restaurants.html">Restaurants</a></p>
							<p>Copyright Â© 2012 grubraise</p>
							</footer>
		
		
		
		<!-- / End Content --></section>
		
		
		
		
		<!-- / End Wrap --></section>
		<!-- / ********************************************************************************************************************************************************************* -->

		
		
		
		
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="assets/vendor/jquery-1.8.2.min.js"><\/script>')</script>

		
		<script src="assets/jquery.metadata.js" type="text/javascript"></script>  
		<script src="assets/jquery.validate.js" type="text/javascript"></script>
        <script src="assets/main.js"></script>

       <script>
